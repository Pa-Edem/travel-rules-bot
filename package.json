{
  "name": "travel-rules-bot",
  "version": "0.1.0",
  "type": "module",
  "description": "Telegram bot with verified information about travel rules and regulations",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.{ts,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,json}\""
  },
  "keywords": [
    "telegram",
    "bot",
    "travel",
    "rules",
    "regulations"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=22.18.0"
  },
  "dependencies": {
    "@grammyjs/conversations": "^2.1.1",
    "@grammyjs/i18n": "^1.1.2",
    "@supabase/supabase-js": "^2.45.4",
    "dotenv": "^16.4.5",
    "grammy": "^1.30.0",
    "winston": "^3.19.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.1",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "eslint": "^9.15.0",
    "prettier": "^3.3.3",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
